{
    "name": "@ionaru/web-server",
    "version": "0.0.2",
    "description": "My package web-server",
    "bin": "dist/index.js",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "tsc --project tsconfig.json",
        "lint:src": "tslint --project tsconfig.json -c tslint.json src/**/*.ts --format stylish",
        "lint:spec": "tslint --project tsconfig.json -c tslint.json test/**/*.ts --format stylish",
        "lint": "npm run lint:src && npm run lint:spec",
        "pretest": "npm run lint",
        "test": "jest",
        "prepare": "npm run build",
        "preversion": "npm run test"
    },
    "jest": {
        "collectCoverage": true,
        "preset": "ts-jest",
        "testEnvironment": "node"
    },
    "keywords": [],
    "author": "Jeroen Akkerman",
    "repository": "github:Ionaru/web-server",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^24.0.6",
        "@types/node": "latest",
        "jest": "^24.1.0",
        "ts-jest": "^24.0.0",
        "tslint": "latest",
        "typescript": "latest"
    },
    "dependencies": {
        "@ionaru/typed-events": "0.0.1"
    }
}
